<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
</head>
<body>
    <h1>Product List</h1>
    <ul>
        {{#each products}}
        <li>
            <h3>{{this.id}}</h3>
            <h3>{{this.title}}</h3>
            <h3>{{this.description}}</h3>
            <h3>{{this.price}}</h3>
            <h3>{{this.thumbnail}}</h3>
            <h3>{{this.code}}</h3>
            <h3>{{this.stock}}</h3>
        </li>
        {{/each}}
    </ul>
</body>
</html>